                    }
                },
                "type": "object"
            },
            "ResponseSignupInvalidData": {
                "title": "Response Signup Invalid Data",
                "description": "회원가입 입력 정규식 오류메시지",
                "properties": {
                    "error": {
                        "$ref": "#/components/schemas/RegexMessages"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearer_token": {
                "type": "apiKey",
                "description": "입력형식: Bearer {{access_token}}",
                "name": "Authorization",
                "in": "header"
            }
        }
    },
    "security": [
        []
    ]
}
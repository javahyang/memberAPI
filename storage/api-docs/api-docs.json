                    "gender": {
                        "title": "gender",
                        "description": "성별: 남성(M), 여성(F)",
                        "type": "string",
                        "example": "M"
                    }
                },
                "type": "object"
            },
            "RequestUsers": {
                "title": "Request Users",
                "description": "회원검색 요청 데이터",
                "properties": {

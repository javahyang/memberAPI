    "servers": [
        {
            "url": "http://localhost",
            "description": "로컬 테스트 서버"
        }
    ],
    "paths": {
        "/api/orders/details": {
            "get": {
                "tags": [
                    "주문"
                ],
                "summary": "주문 목록조회",
                "description": "로그인한 회원의 주문 목록조회 API",
                "responses": {
                    "200": {
                        "description": "로그인한 회원의 주문 목록을 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseOrdersDetails"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/api/signup": {
            "post": {
                "tags": [
                    "회원"
                ],
                "summary": "신규 회원가입",
                "description": "회원가입 API",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RequestSignup"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "회원가입 후, token 과 user 를 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseSignup"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "정규식에 맞지 않는 필드명과 오류메시지를 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseSignupInvalidData"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/signin": {
            "post": {
                "tags": [
                    "회원"
                ],
                "summary": "로그인",
                "description": "로그인 API",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RequestSignin"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "로그인 후, token 을 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseSignin"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "정규식에 맞지 않는 필드명과 오류메시지를 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseSigninInvalidData"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "로그인 정보 오류메시지를 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseSigninFail"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/users/details": {
            "get": {
                "tags": [
                    "회원"
                ],
                "summary": "회원 상세정보",
                "description": "회원 상세정보 조회 API",
                "responses": {
                    "200": {
                        "description": "로그인한 회원정보를 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseUsersDetails"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        },
        "/api/users": {
            "get": {
                "tags": [
                    "회원"
                ],
                "summary": "회원목록",
                "description": "회원목록 조회 API",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "페이지네이션의 페이지 넘버",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        },
                        "example": 1
                    }
                ],
                "responses": {
                    "200": {
                        "description": "회원목록을 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseUsers"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/users/search": {
            "post": {
                "tags": [
                    "회원"
                ],
                "summary": "회원검색",
                "description": "회원검색(이름, 이메일) API",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RequestUsers"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "이름, 이메일로 검색한 회원정보를 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseUsers"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "정규식에 맞지 않는 필드명과 오류메시지를 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseUsersInvalidData"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/signout": {
            "get": {
                "tags": [
                    "회원"
                ],
                "summary": "로그아웃",
                "description": "로그아웃 API",
                "responses": {
                    "200": {
                        "description": "로그아웃 메시지를 반환합니다.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseSignout"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearer_token": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "Order": {
                "title": "Order",
                "description": "Order 데이터",
                "properties": {
                    "id": {
                        "title": "id",
                        "description": "Id",
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "email": {
                        "title": "email",
                        "description": "회원 이메일주소(아이디)",
                        "type": "string",
                        "example": "gildong@test.com"
                    },
                    "order_number": {
                        "title": "order_number",
                        "description": "주문번호",
                        "type": "string",
                        "example": "QMAYTOPQN6R0"
                    },
                    "product_name": {
                        "title": "product_name",
                        "description": "제품명",
                        "type": "string",
                        "example": "수제 인센스 홀더 🦉"
                    },
                    "paid_at": {
                        "title": "paid_at",
                        "description": "결제일시",
                        "type": "string",
                        "format": "date-time",
                        "example": "2021-03-12T10:01:57.000000Z"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "Order"
                }
            },
            "User": {
                "title": "User",
                "description": "User 데이터",
                "properties": {
                    "id": {
                        "title": "id",
                        "description": "Id",
                        "type": "integer",
                        "format": "int64",
                        "example": 1
                    },
                    "name": {
                        "title": "name",
                        "description": "회원 이름",
                        "type": "string",
                        "example": "홍길동"
                    },
                    "email": {
                        "title": "email",
                        "description": "회원 이메일주소(아이디)",
                        "type": "string",
                        "example": "gildong@test.com"
                    },
                    "nickname": {
                        "title": "nickname",
                        "description": "닉네임",
                        "type": "string",
                        "example": "hongho"
                    },
                    "phone_number": {
                        "title": "phone_number",
                        "description": "전화번호",
                        "type": "string",
                        "example": "01012342345"
                    },
                    "gender": {
                        "title": "gender",
                        "description": "성별: 남성(M), 여성(F), null",
                        "type": "string",
                        "example": "M"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "User"
                }
            },
            "RequestSignin": {
                "title": "Request Signin",
                "description": "로그인 요청 데이터",
                "properties": {
                    "email": {
                        "title": "email",
                        "description": "이메일주소: 이메일형식",
                        "type": "string",
                        "example": "gildong@test.com"
                    },
                    "password": {
                        "title": "password",
                        "description": "비밀번호: 영어 대문자, 소문자, 특수문자(@^$!%*?&), 숫자가 각 1회 이상씩 포함된 10자리 이상",
                        "type": "string",
                        "example": "Bawen&a1qq"
                    }
                },
                "type": "object"
            },
            "RequestSignup": {
                "title": "Request Signup",
                "description": "회원가입 요청 데이터",
                "required": [
                    "name",
                    "email",
                    "password",
                    "c_password",
                    "nickname",
                    "phone_number"
                ],
                "properties": {
                    "name": {
                        "title": "name",
                        "description": "이름: 한글, 영어 대문자/소문자",
                        "type": "string",
                        "example": "홍길동"
                    },
                    "email": {
                        "title": "email",
                        "description": "이메일주소: 이메일형식",
                        "type": "string",
                        "example": "gildong@test.com"
                    },
                    "password": {
                        "title": "password",
                        "description": "비밀번호: 영어 대문자, 소문자, 특수문자(@^$!%*?&), 숫자가 각 1회 이상씩 포함된 10자리 이상",
                        "type": "string",
                        "example": "Bawen&a1qq"
                    },
                    "c_password": {
                        "title": "c_password",
                        "description": "비밀번호 확인: password 필드와 같은 값",
                        "type": "string",
                        "example": "Bawen&a1qq"
                    },
                    "nickname": {
